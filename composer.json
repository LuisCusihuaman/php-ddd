{
  "name": "luiscusihuaman/php-ddd",
  "license": "MIT",
  "type": "project",
  "description": "DDD php Skeleton - Monorepository",
  "require": {
    "php": "^7.4",
    "ext-apcu": "*",
    "ext-json": "*",
    "ext-amqp": "*",
    "ext-zend-opcache": "*",
    "ext-pdo": "*",
    "symfony/framework-bundle": "^4.3",
    "symfony/dotenv": "^4.3",
    "symfony/yaml": "^4.3",
    "symfony/cache": "^5.3",
    "doctrine/orm": "^2.9",
    "doctrine/dbal": "2.13.2",
    "doctrine/cache": "^1.8",
    "lambdish/phunctional": "^2.0",
    "symfony/messenger": "^4.4",
    "symfony/http-foundation": "5.2"
  },
  "require-dev": {
    "ext-xdebug": "*",
    "roave/security-advisories": "dev-master",
    "behat/behat": "^3.5",
    "behat/mink-extension": "^2.3",
    "behat/mink-browserkit-driver": "^1.3",
    "friends-of-behat/symfony-extension": "^2.0",
    "phpunit/phpunit": "^9.5",
    "ramsey/uuid": "^3.8",
    "fzaninotto/faker": "1.8",
    "mockery/mockery": "^1.4"
  },
  "autoload": {
    "psr-4": {
      "LuisCusihuaman\\Apps\\Mooc\\Backend\\": "apps/mooc/backend/src",
      "LuisCusihuaman\\Apps\\Mooc\\Frontend\\": "apps/mooc/frontend/src",
      "LuisCusihuaman\\Apps\\Backoffice\\Backend\\": "apps/backoffice/backend/src",
      "LuisCusihuaman\\Apps\\Backoffice\\Frontend\\": "apps/backoffice/frontend/src",
      "LuisCusihuaman\\": [
        "src"
      ]
    }
  },
  "autoload-dev": {
    "psr-4": {
      "LuisCusihuaman\\Tests\\": [
        "tests/src"
      ]
    }
  }
}
